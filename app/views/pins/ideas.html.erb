<div class="menu-button">
	<h1>Ideas</h1>
	
	<div id="pins" class="transitions-enabled">
	  <% @pins.each do |pin| %>
	    <div class="box panel panel-default">
	      <%= link_to (image_tag pin.image.url(:medium)), pin %>
	      <div class="panel-body">
	        <p><%= pin.description %></p>
	        <p><%= pin.user.name if pin.user %></p>
	      </div>
	    </div>
	  <% end %>
	</div>

</div>